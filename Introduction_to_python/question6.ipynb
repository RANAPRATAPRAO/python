    "def is_leap(year):\n",
    "    leap = False\n",
    "    \n",
    "    # Write your logic here\n",
    "    leap=(year%400==0)or(year%4==0 and year%100!=0)\n",
    "    \n",
    "    return leap\n",
    "\n",
    "year=int(input())\n",
    "print(is_leap(year))"
   ]
